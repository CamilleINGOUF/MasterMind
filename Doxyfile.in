PROJECT_NAME           = "@CMAKE_PROJECT_NAME@"
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ \
		       	 @PROJECT_BINARY_DIR@
INPUT                  = @PROJECT_SOURCE_DIR@/src \
		         @PROJECT_SOURCE_DIR@/include
OUTPUT_DIRECTORY       = @PROJECT_BINARY_DIR@/doc_doxygen
FILE_PATTERNS	       = *.hpp \
		       	 *.cpp
EXTRACT_ALL            = YES
RECURSIVE              = YES
GENERATE_LATEX 	       = NO
OUTPUT_LANGUAGE        = French